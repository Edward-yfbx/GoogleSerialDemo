<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.serial">

    <application
        android:name="com.google.serial.sample.Application"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.google.serial.sample.MainMenu"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.serial.sample.SerialPortPreferences" />
        <activity android:name="com.google.serial.sample.ConsoleActivity" />
        <activity android:name="com.google.serial.sample.LoopbackActivity" />
        <activity android:name="com.google.serial.sample.Sending01010101Activity" />
    </application>

</manifest>